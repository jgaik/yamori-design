{
  "name": "@yamori-design/styles",
  "version": "2.10.2",
  "description": "Yamori Design stylesheets files",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css",
    "dist/**/*.css"
  ],
  "scripts": {
    "build": "npm run clean && sass --no-source-map src:dist",
    "dev": "sass --no-source-map --watch src:dist",
    "clean": "rimraf .turbo dist"
  },
  "author": "Jakub Gaik",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.80.3"
  }
}
